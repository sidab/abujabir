{
  "name": "com.abujabir.ml",
  "version": "1.4.0",
  "displayName": "Единобожие - Вероубеждение",
  "cordova": {
    "platforms": [
      "browser",
      "android"
    ],
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-crosswalk-webview-v2": {
        "XWALK_VERSION": "23+",
        "XWALK_LITEVERSION": "xwalk_core_library_canary:17+",
        "XWALK_COMMANDLINE": "--disable-pull-to-refresh-effect",
        "XWALK_MODE": "embedded",
        "XWALK_MULTIPLEAPK": "true"
      },
      "cordova-plugin-build-architecture": {}
    }
  },
  "dependencies": {
    "cordova-android": "^7.1.4",
    "cordova-browser": "^5.0.4",
    "cordova-plugin-build-architecture": "1.0.6",
    "cordova-plugin-crosswalk-webview-v2": "2.4.3",
    "cordova-plugin-splashscreen": "~5.0.2",
    "cordova-plugin-whitelist": "~1.3.3"
  }
}