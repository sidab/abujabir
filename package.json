{
  "name": "com.abujabir.ml",
  "version": "1.4.0",
  "displayName": "Единобожие - Вероубеждение",
  "cordova": {
    "platforms": [
      "android",
      "browser",
      "ios"
    ],
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-x-socialsharing": {},
      "cordova-android-support-gradle-release": {
        "ANDROID_SUPPORT_VERSION": "27.+"
      },
      "cordova-plugin-crosswalk-webview": {
        "XWALK_MODE": "lite",
        "XWALK_VERSION": "23+",
        "XWALK_LITEVERSION": "xwalk_core_library_canary:17+",
        "XWALK_COMMANDLINE": "--disable-pull-to-refresh-effect",
        "XWALK_MULTIPLEAPK": "true"
      },
      "cordova-build-architecture": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-wkwebview-engine": {},
      "cordova-plugin-wkwebview-file-xhr": {},
      "cordova-plugin-background-audio": {},
      "cordova-plugin-background-mode": {},
      "cordova-plugin-music-controls": {}
    }
  },
  "dependencies": {
    "cordova-android": "^8.1.0",
    "cordova-android-support-gradle-release": "3.0.1",
    "cordova-browser": "^6.0.0",
    "cordova-build-architecture": "git+https://github.com/MBuchalik/cordova-build-architecture.git#v1.0.4",
    "cordova-ios": "^5.0.1",
    "cordova-plugin-audio-background-mode": "0.0.1",
    "cordova-plugin-background-audio": "1.1.0",
    "cordova-plugin-background-mode": "0.7.3",
    "cordova-plugin-crosswalk-webview": "2.4.0",
    "cordova-plugin-device": "2.0.3",
    "cordova-plugin-music-controls": "git+https://github.com/homerours/cordova-music-controls-plugin.git",
    "cordova-plugin-splashscreen": "~5.0.2",
    "cordova-plugin-whitelist": "~1.3.3",
    "cordova-plugin-wkwebview-engine": "1.2.1",
    "cordova-plugin-wkwebview-file-xhr": "2.1.1",
    "cordova-plugin-x-socialsharing": "5.4.0",
    "es6-promise-plugin": "4.2.2"
  },
  "devDependencies": {
    "cordova-plugin-statusbar": "^2.4.3"
  }
}